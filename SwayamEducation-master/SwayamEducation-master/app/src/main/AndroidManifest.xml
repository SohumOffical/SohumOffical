<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sngs.swayam.edu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.attendance.view.StudentAttendanceActivity"></activity>
        <activity
            android:name=".ui.promo.view.PromoEditActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.promo.view.ViewPromosActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.promo.view.CreatePromoActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.student.view.StudentActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.promo.view.PromoActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.dashboard.view.DashboardActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ui.intro.view.IntroActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>